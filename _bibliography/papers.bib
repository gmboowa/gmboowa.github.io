  title      = {Cough Aerosol Cultures of Mycobacterium tuberculosis: Insights on TST / IGRA Discordance and Transmission Dynamics},
  volume     = {10(9)},
  issn       = {e0138358},
  shorttitle = {},
  url        = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0138358},
  doi        = {10.1371/journal.pone.0138358},
  html       = {https://journals.plos.org/plosone/article?id=10.1371%2Fjournal.pone.0138358},
