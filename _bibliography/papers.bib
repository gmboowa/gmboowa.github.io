  urldate    = {2024-12-14},
  journal    = {Microbial Pathogenesis},
  author     = {Musinguzi, Benson and Akampurira, Andrew and Derick, Hope and Turyamuhika, Laban and Mwesigwa, Alex and Mwebesa, Edson and Mwesigye, Vicent and Kabajulizi, Immaculate and Sekulima, Tahalu and Ocheng, Francis and Itabangi, Herbert and Mboowa, Gerald and Sande, James Obondo and Achan, Beatrice},
