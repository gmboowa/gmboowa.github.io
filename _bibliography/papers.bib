  @article{tuberculosis_2014,
  abbr       = {BMC Clin. Pathol.},
  title      = {Clinico-pathological features of tuberculosis due to Mycobacterium tuberculosis Uganda genotype in patients with tuberculous lymphadenitis: a cross sectional study},
  volume     = {14},
  issn       = {14},
  shorttitle = {},
  url        = {https://link.springer.com/article/10.1186/1472-6890-14-14},
  doi        = {10.1186/1472-6890-14-14},
  html       = {https://link.springer.com/article/10.1186/1472-6890-14-14},
