}

@article{amr_2024,
  abbr       = {NMNI},
  title      = {Whole genome-based characterization of extended-spectrum β-lactamase-producing Enterobacter cloacae from orthopedic patients and environment of a tertiary referral hospital in Tanzania},
  volume     = {62},
  issn       = {101486},
  shorttitle = {},
  url        = {https://www.sciencedirect.com/science/article/pii/S2052297524002701},
  doi        = {10.1016/j.nmni.2024.101486},
  html       = {https://doi.org/10.1016/j.nmni.2024.101486},
  google_scholar_id = {PR6Y55bgFSsC},
  pdf={Whole-genome-based_characterization_of_extended-spectrum-β-lactamase-producing_Enterobacter_cloacae_from_orthopedic_patients_and_environment_of_a_tertiary_referral_hospital_in_Tanzania.pdf},
  abstract   = {Objectives: We investigated the genomic epidemiology of extended-spectrum β-lactamase-producing Enterobacter cloacae (ESBL-Ec) isolates from patients and hospital environment to better understand their distribution to help devising effective strategies for infection prevention and control. Methods: We screened ESBL-Ec at Bugando Medical Center (BMC) in Mwanza, Tanzania. Rectal swabs from orthopedic patients on admission and swabs from the neighboring inanimate environment were collected. Following microbial culture, DNA was extracted from pure ESBL-Ec, and whole-genome sequencing was done. Sequence typing (ST), plasmid replicons, drug resistance, and virulence genes were deciphered using the Rapid Microbial Analysis Pipeline (rMAP). Results: We obtained 209 ESBL isolates, of which 15 (7.2 %) were ESBL-Ec [8 (53.3 %) from patients and 7 (46.7 %) from the environment]. Seven isolates were novel and eight were diverse, each with a unique ST. All isolates harbored two to five β-lactamase genes, with the predominance of bla CTX-M-15 (15/15), bla OXA-1 (14/15), bla TEM (14/15) and bla ACT (12/15). The most common non β-lactam drug resistance genes were aac(3)-IIa (14/15), aac(6')-Ib-cr (14/15), fosA (14/15), and qnrB1 (12/15), aph(3″)-Ib (10/15) and aph(6)-Id (10/15). Eleven different types of plasmid replicons were identified in 14/15 of the isolates, harboring one to five plasmids, with the most common plasmids being IncFII (11/15) and IncFIB (10/15). All isolates harbored the outer membrane protein (ompA), and curli protein (csg) was in 14/15 isolates. Conclusion: Admitted orthopedic patients and the hospital environment act as a reservoir of ESBL-Ec with diverse STs and endowed with drug resistance and arsenals of virulence genes, calling for their routine screening on admission for mitigation of potential subsequent infections.},
  number     = {1},
  urldate    = {2024-09-21},
  journal    = {New Microbes and New Infections},
  author     = {Kidenya, Benson R and Mboowa, Gerald and Sserwadda, Ivan and Kanyerezi, Stephen and Nakafu, Esther and Akaro, Lawrence Inyasi and Mkinze, Baraka and Joloba, Moses L and Seni, Jeremiah},
  month      = Sep,
  year       = {2024},
  altmetric  = {},
  dimensions = {true},
  keywords   = {Drug resistance genes, Enterobacter cloacae, Extended-spectrum β-lactamases, Genomes, Plasmid replicon, Sequence type, Tanzania, Virulence genes, Whole genome sequencing (WGS)},
  pages      = {7},
  selected   = {true}

}

@article{amr_2023,
  abbr       = {Hosp Infect},
  title      = {Virulence genes and plasmid replicon profiles of selected β-lactamase-producing Acinetobacter baumannii from orthopaedic patients and the environment in a tertiary referral hospital in Tanzania, East Africa},
  volume     = {141},
  issn       = {223-226},
  shorttitle = {},
  url        = {https://www.journalofhospitalinfection.com/article/S0195-6701(23)00292-X/fulltext},
  doi        = {10.1016/j.jhin.2023.09.005},
  html       = {https://doi.org/10.1016/j.jhin.2023.09.005},
  google_scholar_id = {D_sINldO8mEC},
  pdf={Virulence_genes_and_plasmid_replicon_profiles_of_selected-b-lactamase-producing_Acinetobacter_baumannii_from_orthopaedic_patients_and_the_environment_in_a_tertiary_referral_hospital_in_Tanzania.pdf},
  abstract   = {Acinetobacter baumannii has emerged as an important nosocomial pathogen due to its high resistance to multi-drugs and disinfectants plus its ability to survive in hospital environments. Rectal swabs were collected for screening β-lactamases-producing Acinetobacter baumannii among hospitalized orthopedic patients at a tertiary referral hospital in Tanzania. Swabs were also taken from patients' caretakers, healthcare workers, and the neighboring inanimate environment. A total of 26 confirmed β-lactamases producing Acinetobacter baumannii were isolated, of which 4 representative isolates (two from patients and two from hospital environment) underwent whole-genome sequencing (WGS) to detect sequence types (ST), β-lactamases genes, plasmid replicon types, and virulence genes. All four isolates harbored multiple β-lactamases genes including blaADC-25(3), blaOXA(4), blaCTX-M-15(2) and blaNDM-1(2). Furthermore, isolates harbored virulence genes encoding outer membrane protein (ompA), curli protein (csg), siderophore biosynthesis systems (enterobactin [entABCDEFS, fepABCDG, fes]; yersiniabactin [ybtAEPQSTUX, irp1, irp2, fyuA] and aerobactin [iucABCD, iutA]), transport secretion system type II (T2SS) and type III (T3SS), E. coli common pilus (ecpRABCDE operon), type 1 fimbriae (fim), arylsulfatase (aslA) and adhesions (fedC). Only isolates from patients harbored 4 plasmid replicons each, with the most common plasmid replicons being IncFIA_1; IncY_1 and IncFIB(AP001918)_1. Admitted orthopedic patients and the hospital environment act as a reservoir of multiple β-lactamases producing Acinetobacter baumannii (including those against carbapenems like blaOXA and blaNDM-1) endowed with virulence genes, highlighting the necessity to routinely screening of orthopedic patients with open fractures on admission as well as reinforcing infection prevention and control measures to reduce the dissemination of nosocomial infection within the hospital environment.},
  number     = {1},
  urldate    = {2023-09-18},
  journal    = {Journal of Hospital Infection},
  author     = {Kidenya, Benson R and Mboowa, Gerald and Sserwadda, Ivan and Kanyerezi, Stephen and Nakafu, Esther and Akaro, Lawrence Inyasi and Mkinze, Baraka and Joloba, Moses L and Seni, Jeremiah},
  month      = Sep,
  year       = {2023},
  altmetric  = {},
  dimensions = {true},
  keywords   = {Acinetobacter baumannii, Plasmid replicon, Tanzania, Virulence},
  pages      = {3},
  selected   = {true}

}

@article{cholera_2024,
  abbr       = {MedRxiv},
  title      = {Multicountry genomic analysis underscores regional cholera spread in Africa},
  volume     = {24317392v1},
  issn       = {},
  shorttitle = {},
  url        = {https://www.medrxiv.org/content/10.1101/2024.11.15.24317392v1},
  doi        = {10.1101/2024.11.15.24317392},
  html       = {https://doi.org/10.1101/2024.11.15.24317392},
  google_scholar_id = {JQOojiI6XY0C},
  pdf={Multicountry_genomic_analysis_underscores_regional_cholera_spread_in_Africa.pdf},
  abstract   = {Cholera remains a significant public health burden in many countries in sub-Saharan Africa, though the exact mechanisms of bacterial emergence and spread remain largely undefined. We generated genomic data from 728 Vibrio cholerae O1 isolates predominantly collected between 2019-2024 to create the largest dataset of V. cholerae genomes sequenced locally in Africa. This dataset enabled us to interrogate recent patterns of spread, including the rapid circulation of the AFR15 lineage associated with unusually large outbreaks in Southern Africa. We provide evidence for the movement of the AFR15 lineage into new African Member States and confirm previously observed differences in V. cholerae transmission dynamics in West versus East Africa, though cross-border transmission is prevalent on both sides of the continent. Despite observed differences, evolutionary processes are similar across lineages and we find no evidence for significant changes in antimicrobial resistance genotypes. Overall, our findings emphasize the importance of regionally coordinated cross-border surveillance and interventions, while also demonstrating the critical role of locally generated genomic data in understanding the spread of cholera in Africa.},
  number     = {1},
  urldate    = {2024-11-15},
  journal    = {MedRxiv},
  author     = {Mboowa, Gerald and Matteson, Nathaniel Lucero and Tanui, Collins Kipngetichi and Kasonde, Mpanga and Kamwizik, Guyguyu Kusanzangana and Akanbi, Olusola Anuoluwapo and Chitio, Elias Jucunú Johane and Kagoli, Mathews and Essomba, René Ghislain and Ayitewala, Alisen and Ssewanyana, Isaac and Ngo Bitoungui, Valentina Josiane and Amuri, Aziza Adrienne and Azman, Andrew S and Babatunde, Olajumoke Atinuke and Akenji, Blaise Mboringong and Broban, Anaïs and Malembaka, Espoir Bwenge and Ongole, Francis and Chukwu, Chimaobi Emmanuel and Ismael, Nália and Kapona, Otridah and Laurindo, Osvaldo and Mbala, Placide Kingebeni and Mballa, Georges Alain Etoundi and Miambo, Imelda Carlos Zulfa and Mwanyongo, Alex Ansaye and Najjuka, Grace and Mutale, Joseph and Musonda, Kunda and Niyonzima, Allan Muruta and Nyenje, Mirriam Ethel and Popoola, Michael and Shempela, Doreen Mainza and Sike, Christiane Medi and Sitoe, Sofião Manjor and Wanjohi, Dorcas Waruguru and Welo, Placide Okitayemba and Yelewa, Mtisunge and Yennan, Sebastian and Ziba, Lucius and Consortium, CholGEN and Bitilinyu-Bangoh, Joseph Ephram and Chilengi, Roma and Hadja, Hamsatou and Idris, Jide and Langa, José Paulo Maurício and Mukadi-Bamuleka, Daniel and Nabadda, Susan and Debes, Amanda K and Sack, David A and Kaseya, Jean and Kebede, Yenew Tebeje and Wohl, Shirlee and Tessema, Sofonias Kifle},
