@article{amr_2014,
  abbr       = {BMC Infect Dis},
  title      = {Rifampicin resistance mutations in the 81 bp RRDR of rpoB gene in Mycobacterium tuberculosis clinical isolates using Xpert® MTB/RIF in Kampala, Uganda: a retrospective study},
  volume     = {14},
  issn       = {481},
  shorttitle = {},
  url        = {https://bmcinfectdis.biomedcentral.com/articles/10.1186/1471-2334-14-481},
  doi        = {10.1186/1471-2334-14-481},
  html       = {https://link.springer.com/article/10.1186/1471-2334-14-481},
  google_scholar_id = {UeHWp8X0CEIC},
  pdf={Rifampicin_resistance_mutations_in_the_81_bp_RRDR_of_rpoB_gene_in_Mycobacterium_tuberculosis_clinical_isolates_using_Xpert.pdf},
  abstract   = {Background: Introduction of Xpert® MTB/RIF assay has revolutionalised the diagnosis of tuberculosis (TB) by simultaneously detecting the bacteria and resistance to rifampicin (rif), a surrogate marker for multi-drug resistant TB (MDR-TB) as well as one of the principal first-line anti-tuberculosis drugs. In general, rpoB mutations can be found in 96.1% of rif-resistant Mycobacterium tuberculosis (MTB) strains worldwide and these mutations usually are located in a region at the 507-533rd amino acid residuals (81 bp) in the MTB rpoB gene, which is referred to as Rifampicin-resistance-determining region (RRDR). In this study, we determined the frequency of MDR-TB in Kampala using Xpert® MTB/RIF in comparison with the agar proportion method using Middlebrook 7H11and further determined the frequency of probes for different rpoB gene mutations using Xpert® MTB/RIF assay in the 81 bp RRDR. Methods: A total of 1501 specimens received at Mycobacteriology laboratory, Makerere University for Xpert testing between May 2011 and May 2014 were analysed by Xpert® MTB/RIF assay. Specimens that were positive for both MTB and rifampicin resistance were further subjected to a complete first line anti-mycobacterial drug susceptibility testing using Middlebrook 7H11 agar proportion method (APM). Results: Xpert® MTB/RIF assay detected 313 MTB positive specimens and out of which 12 specimens had both MTB and rifampicin- resistance conferred by four different rpoB gene mutations in the 81 bp-RRDR of MTB, further one (1/12), specimen was found to be rifampicin mono-resistant on APM while the 11 were found to be MDR-TB. Probes associated with the observed rif- resistance were as follows: E (7/12), B (3/12), A (1/12), D (1/12) and no rif-resistance was associated with probe C. No specimen yielded rif-resistance associated with more than one probe failure (mutation combinations). Probe D was associated with rifampicin mono-resistant. Conclusions: MDR-TB was at 3.5% in the studied population. Mutations associated with Probe E (58%) also known as codons 531and 533 are the commonest rpoB gene mutation identified by Xpert® MTB/RIF assay in this setting and mutations identified by probe E of the assay, turned out to be MDR-TB strains by agar proportion method antimicrobial susceptibility testing. No mutation was detected in the codon 522.},
  number     = {1},
  urldate    = {2014-09-04},
  journal    = {BMC Infectious Diseases},
  author     = {Mboowa, Gerald and Namaganda, Carolyn and Ssengooba, Willy},
  month      = Sep,
  year       = {2014},
  altmetric  = {},
  dimensions = {true},
  keywords   = {Rifampicin-resistance-determining region, RNA polymerase B gene, Multi-drug Resistant tuberculosis, Xpert® MTB/RIF},
  pages      = {5},
  selected   = {true}

}

@article{amr_2014,
  abbr       = {Bioinformatics},
  title      = {Bioinformatics mentorship in a resource limited setting},
  volume     = {23},
  issn       = {1},
  shorttitle = {},
  url        = {https://academic.oup.com/bib/article/23/1/bbab399/6377513},
  doi        = {10.1093/bib/bbab399},
  html       = {https://academic.oup.com/bib/article/23/1/bbab399/6377513},
  google_scholar_id = {J_g5lzvAfSwC},
  pdf={Bioinformatics_mentorship_in_a_resource_limited_setting.pdf},
  abstract   = {Background: The two recent simultaneous developments of high-throughput sequencing and increased computational power have brought bioinformatics to the forefront as an important tool for effective and efficient biomedical research. Consequently, there have been multiple approaches to developing bioinformatics skills. In resource rich environments, it has been possible to develop and implement formal fully accredited graduate degree training programs in bioinformatics. In resource limited settings with a paucity of expert bioinformaticians, infrastructure and financial resources, the task has been approached by delivering short courses on bioinformatics—lasting only a few days to a couple of weeks. Alternatively, courses are offered online, usually over a period of a few months. These approaches are limited by both the lack of sustained in-person trainer–trainee interactions, which is a key part of quality mentorships and short durations which constrain the amount of learning that can be achieved. Methods: Here, we pioneered and tested a bioinformatics training/mentorship model that effectively uses the available expertise and computational infrastructure to deliver an in-person hands-on skills training experience. This is done through a few physical lecture hours each week, guided personal coursework over the rest of the week, group discussions and continuous close mentorship and assessment of trainees over a period of 1 year. Results: This model has now completed its third iteration at Makerere University and has successfully mentored trainees, who have progressed to a variety of viable career paths. Conclusions: One-year (intermediate) skills based in-person bioinformatics training and mentorships are viable, effective and particularly appropriate for resource limited settings.},
  number     = {1},
  urldate    = {2021-09-30},
  journal    = {Briefings in Bioinformatics},
  author     = {Jjingo, Daudi and Mboowa, Gerald and Sserwadda, Ivan and Kakaire, Robert and Kiberu, Davis and Amujal, Marion and Galiwango, Ronald and Kateete, David and Joloba, Moses and Whalen, Christopher C},
  month      = Sep,
  year       = {2021},
  altmetric  = {},
  dimensions = {true},
  keywords   = {bioinformatics, education, computational biology, mentorship, training, intermediate},
  pages      = {8},
  selected   = {true}

}

@article{pcr_2024,
  abbr       = {JCM},
  title      = {CDC Trioplex diagnostic assay underperforms in detection of circulating Chikungunya West African genotype},
  volume     = {62},
  issn       = {7},
  shorttitle = {},
  url        = {https://journals.asm.org/doi/10.1128/jcm.00405-24},
  doi        = {10.1128/jcm.00405-24},
  html       = {https://journals.asm.org/doi/epub/10.1128/jcm.00405-24},
  google_scholar_id = {LjlpjdlvIbIC},
  pdf={ndiaye-et-al-2024-cdc-trioplex-diagnostic-assay-underperforms-in-detection-of-circulating-chikungunya-west-african.pdf},
  abstract   = {Since 2015, the virology laboratory at Institut Pasteur de Dakar (IPD) has conducted real-time molecular diagnostics for arboviruses as part of Senegal's syndromic sentinel surveillance network. During the largest Chikungunya fever (CF) outbreak in Senegal in late 2023, with 210 confirmed cases in Kédougou and Tambacounda, the IPD evaluated the CDC Trioplex RT-qPCR assay, which detects Zika, Dengue, and Chikungunya viruses. Testing revealed a significant performance delay—an average 7 Ct value—compared to IPD’s in-house assay for 15 CHIKV RNA-positive samples. Whole genome sequencing and in silico analyses identified seven mismatches in the CDC Trioplex assay’s primer and probe binding sites for the CHIKV West Africa (WA) genotype, notably a critical mismatch at the 3’ end of the forward primer. This mismatch reduced elongation efficiency and caused a detection shift of approximately two logs, raising the risk of false negatives, especially for samples with low viral loads (Ct > 28). Conversely, the IPD in-house assay remained effective, with no mismatches detected against CHIKV WA genotype sequences. The findings highlight the need for continuous genomic surveillance and assay validation using region-specific viral strains to ensure diagnostic accuracy during outbreaks.},
  number     = {1},
  urldate    = {2024-06-13},
  journal    = {Journal of Clinical Microbiology},
  author     = {Ndiaye, Mignane and Kane, Mouhamed and Balde, Diamilatou and Sankhé, Safietou and Mbanne, Maimouna and Diop, Seynabou Mbaye Souna and Ahmad, Umar and Mboowa, Gerald and Sagne, Samba Niang and Cisse, Mamadou and Dia, Ndongo and Sall, Amadou Alpha and Faye, Ousmane and Fall, Gamou and Faye, Oumar and Weidmann, Manfred and Diagne, Moussa Moïse and Dieng, Idrissa},
